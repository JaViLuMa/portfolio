{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "utils/*": ["src/utils/*"],
      "icons/*": ["src/icons/*"],
      "hooks/*": ["src/hooks/*"],
      "constants/*": ["src/constants/*"]
    }
  }
}
